{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "noEmit": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "incremental": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@lib/*": ["lib/*"],
      "@containers/*": ["containers/*"],
      "@atoms/*": ["atoms/*"],
      "@public/*": ["public/*"],
      "@data/*": ["data/*"]
    }
  },
  "include": ["src/next-env.d.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*"],
  "exclude": ["node_modules", "unused", "/unused", "unused/*", "./unused"]
}
